  "author": {
    "name": "Juan M. CaÃ±abate (@juanmait)"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.1.0",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.21",
    "husky": "^1.3.1",
    "jest": "^24.0.0",
    "lint-staged": "^8.1.3",
    "prettier": "^1.16.3",
    "prettier-tslint": "^0.4.2",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.3.1"
